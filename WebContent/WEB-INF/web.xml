<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TPC</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>SignupServlet</servlet-name>
    <servlet-class>tpc.SignupServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SignupServlet</servlet-name>
    <url-pattern>/SignupServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CLogin</servlet-name>
    <servlet-class>tpc.CLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CLogin</servlet-name>
    <url-pattern>/clogin.jsp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>tpc.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ForJobServlet</servlet-name>
    <servlet-class>tpc.ForJobServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ForJobServlet</servlet-name>
    <url-pattern>/ForJobServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ForInternServlet</servlet-name>
    <servlet-class>tpc.ForInternServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ForInternServlet</servlet-name>
    <url-pattern>/ForInternServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>EditDetailsServlet</servlet-name>
    <servlet-class>tpc.EditDetailsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditDetailsServlet</servlet-name>
    <url-pattern>/EditDetailsServlet</url-pattern>
  </servlet-mapping>
</web-app>